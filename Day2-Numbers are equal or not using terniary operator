int main()
{
	int n1,n2;
	cout<<"enter the first number:"<<endl;
	cin>>n1;
	cout<<"enter the second number:"<<endl;
	cin>>n2;
	string condition;
	condition=(n1==n2)?"numbers are equal": "numbers are not equal";
	//cout<<condition;
	return 0;
}
