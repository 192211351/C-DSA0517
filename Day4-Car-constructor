#include<iostream>
#include<string>
using namespace std;
class Car
{
	public:
		string brand;
		string model;
		int year;
		Car(string x,string y,int z)
		{
			brand=x;
			model=y;
			year = z;
		}
};
int main()
{
	Car carobj1("bmw","i",101);
	Car carobj2("mahendra","j",102);
	cout<<carobj1.brand<<" "<<carobj1.model<<" "<<carobj1.year<<endl;
	cout<<carobj2.brand<<carobj2.model<<carobj2.year<<endl;
	return 0;
}

OUTPUT:
bmw i 101
mahendraj102
