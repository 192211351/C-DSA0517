#include<iostream>
using namespace std;
int main()
{
	int a,b;
	cout<<"enter the first number:";
	cin>>a;
	cout<<"enter the second number:";
	cin>>b;
	cout<<a+b<<"\n";
	cout<<a-b<<"\n";
	cout<<a*b<<"\n";
	cout<<a/b<<"\n";
	return 0;
}

OUTPUT:
enter the first number:3
enter the second number:4
7
-1
12
0
