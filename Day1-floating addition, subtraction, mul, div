#include<iostream>
using namespace std;
int main()
{
	float a,b;
	cout<<"enter the first number:\n";
	cin>>a;
	cout<<"enter the second number:\n";
	cin>>b;
	cout<<a+b,"\n";
	cout<<a-b,"\n";
	cout<<a*b,"\n";
	cout<<a/b,"\n";
	return 0;
}
OUTPUT:
enter the first number:
4.0
enter the second number:
5.0
9
-1
20
0.8
