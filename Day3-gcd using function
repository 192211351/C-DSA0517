#include<iostream>
using namespace std;
void gcd()
{
	int n1,n2,Gcd=0,i;
	cout<<"enter the first number:"<<endl;
	cin>>n1;
	cout<<"enter the second number:"<<endl;
	cin>>n2;
	for(i=1;i<n1&&i<n2;i++)
	{
		if(n1%i==0 && n2%i==0)
		{
			Gcd=i;
		}
	}
	cout<<Gcd;
};
int main()
{
	gcd();
	return 0;
}

Output:
enter the first number:
15
enter the second number:
10
5
