#include<iostream>
using namespace std;
void prime()
{
	int n,i,count=0;
	cout<<"enter the number:"<<endl;
	cin>>n;
	for(i=1;i<=n;i++)
	{
		if(n%i==0)
		{
			count++;
		}
	}
	if(count==2)
	cout<<"given number is prime"<<endl;
	else
	cout<<"not a prime number"<<endl;
};
int main()
{
	prime();
	return 0;
}

OUTPUT:
enter the number:
5
given number is prime
